<body>
<script type="text/javascript">
var i =0
var url = "https://raw.githubusercontent.com/robbykraft/CoreInteraction/master/divs.json"

$.getJSON(url, function(data){
	for (var i=0; i<url.length; i= i+1){
		var div = document.createElement("div");
		div.style.top = data[i].top + "px";
		div.style.length = data[i].length + "px"; 
		div.style.width = data[i].width + "px";
		div.style.height = data[i].height + "px";
		document.body.appendChild(div);
		}
	})



// function getData(){
